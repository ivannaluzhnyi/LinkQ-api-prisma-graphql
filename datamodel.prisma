type Account {
  id: Int! @id
  application: [Application]
  birthdate: DateTime
  created: DateTime!
  email: String! @unique
  firstname: String
  guarantor: [Guarantor]
  isActive: Boolean! @db(name: "is_active")
  lastname: String
  password: String!
  property: [Property]
  roles: Json!
  salary: Float
  updated: DateTime!
}

type Address  {
  id: Int! @id
  city: String!
  country: String!
  floor: Int
  property: Property!
  room: Int
  street: String!
  zipcode: String!
}

type Application {
  id: Int! @id
  buyer: [Account]!
  contract: Contract!
  created: DateTime!
  offer: Float!
  property: [Property]!
  updated: DateTime!
}

type Contract{
  id: Int! @id
  application: [Application]!
  created: DateTime!
  price: Float!
  updated: DateTime!
}

type DoctrineMigrationVersion {
  version: ID! @id
  executedAt: DateTime @db(name: "executed_at")
  executionTime: Int @db(name: "execution_time")
}

type Feature {
  id: Int! @id
  bathrooms: Int!
  bedrooms: Int!
  garages: Int!
  property: [Property]!
  rooms: Int!
  size: Int!
}

type Greeting {
  id: Int! @id
  name: String!
}

type Guarantor{
  id: Int! @id
  firstName: String! 
  lastName: String!
  relation: String!
  salary: Float!
  userRelated: [Account]!
}

type Media  {
  id: Int! @id
  created: DateTime!
  extension: String!
  filename: String!
  property: [Property]
  updated: DateTime!
  uri: String!
}

type Property{
  id: Int! @id
  address: [Address]! 
  application: [Application]
  description: String!
  features: [Feature]! 
  media: [Media]
  title: String!
  userRelated: [Account]
}
